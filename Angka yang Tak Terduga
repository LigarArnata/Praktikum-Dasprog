// Angka yang Tak Terduga

#include <stdio.h>
#include <stdlib.h>
#include <math.h>

void deret()
{
  int i, n;
  int n1 = 0, n2 = 3;
  int bilanganselanjutnya = n1 + n2;
  scanf("%d", &n);
  if(n==1){
    printf("%d", n1);
  }else{
  printf("%d %d ", n1, n2);
  for (i = 3; i <= n; ++i) {
    printf("%d ", bilanganselanjutnya);
    n1 = n2;
    n2 = bilanganselanjutnya;
    bilanganselanjutnya = n1 + n2;
  }
}
}

int main()
{
 deret();
  return 0;
}
